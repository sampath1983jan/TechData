    function fnApplyTimeOffForEmployees() {
        var objCallBack = new $$.Ajax.CallBack();
        objCallBack.DoCallBack("LoadEligiblePermissionPolicyTypeList", ClientID + "~" + UserID, "/" + virtualPath + "Handler/TAHandler.ashx");
        objCallBack.OnComplete = function (rt, rx) {
 
            var dtResult = eval(rt);
            if (dtResult.length > 0) {
                TalentOzDynamicScript("Scripts/TimeAndAttendance/PermissionPolicyRequest.js?v=20190221", function (src, Status) {
                    var tb1 = $$.TalentOZ.Tabs.CreateTab({ 'ID': 'div_Apply_Permission_Req' + UserID, 'TabName': 'Apply Time off For Employee' });
                    var PerReq = new PermissionPolicyRequest();
                    PerReq.ClientID = ClientID;
                    PerReq.UserID = UserID;
                    PerReq.LoginRequestUserID = UserID;
                    PerReq.plc = tb1.property.containerid
                    PerReq.PermissionPolicyTypeSrc = dtResult;
					PerReq.IsFromHRApply = 1;
                    PerReq.Init();
                });
            } else {
                var fb = new $$.UI.WebControls.FeedBack();
                fb.AddFeedBack("fbDiv", "div_msg");
                fb.AddMessage("You cannot apply for this time off, as workflow is not been configured. Please contact your system administrator.", 1);
                fb.Bind();
                return false;
            }
        }
    }

