    // Raise Resignation Request 

    function fnRaiseResignation() {
        var objCallBack = new $$.Ajax.CallBack();
        objCallBack.DoCallBack("CheckSeparationAvailability", UserID, "/" + virtualPath + "Handler/SeparationHandler.ashx");
        objCallBack.OnComplete = function (rt, rx) {

            var dtSources = eval("(" + rt + ")");
            if ((dtSources.Table[0]['count(1)'] == 0) && (dtSources.Table2[0]['count(1)'] == 0)) {
                if (dtSources.Table1[0].EmployeeCanRaiseResignationRequest == "1") {
                    var resig = new COM.ComponentDetail();
                    resig.SeparationConfirm(6, dtSources.Table1[0], UserID);
                } else {
                    var fb = new $$.UI.WebControls.FeedBack();
                    fb.AddFeedBack("fbDiv", 'caFeed_N');
                    fb.AddMessage("You are not eligible for raise resignation request, please contact your HR.", 1);
                    fb.Bind();
                    return;
                }
            } else if (dtSources.Table[0]['count(1)'] > 0) {
                if (dtSources.Table3[0].Type == 1) {
                    var fb = new $$.UI.WebControls.FeedBack();
                    fb.AddFeedBack("fbDiv", 'caFeed_N');
                    fb.AddMessage("You already raised a resignation request, so you cannot apply again", 1);
                    fb.Bind();
                    return;
                } else {
                    var fb = new $$.UI.WebControls.FeedBack();
                    fb.AddFeedBack("fbDiv", 'caFeed_N');
                    fb.AddMessage("You already raised a resignation request, so you cannot apply again", 1);
                    fb.Bind();
                    return;
                }

            } else if (dtSources.Table2[0]['count(1)'] > 0) {
                var fb = new $$.UI.WebControls.FeedBack();
                fb.AddFeedBack("fbDiv", 'caFeed_N');
                fb.AddMessage("HR Raised Resignation Request for you, So you cannot apply.", 1);
                fb.Bind();
                return;

            }
        }

    }



