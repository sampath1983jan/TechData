            var tbnew = new $$.UI.WebControls.TabView();
            tbnew.AddTab({ 'ID': id, 'TabName': name }, callback, arg);
            tbnew.id = 'ComponentTab';
            tbnew.enableAddTab = true;
            tb1 = tbnew.Bind('Div1');
            $("#addtb_" + tbnew.id).remove();
            $$.TalentOZ.Tabs = new Object();
            $$.TalentOZ.Tabs = tbnew;
