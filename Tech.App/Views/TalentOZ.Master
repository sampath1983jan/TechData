    function CheckTimeout() {
        var now = new Date();
        if (document.now == undefined) {
            document.now = new Date();
            return;
        }

        document.now = now;

    }
    var intt;
    var stMin = 3;

    var objCallBack = new $$.Ajax.CallBack();
    objCallBack.DoCallBack("getST", -1, "/" + virtualPath + "Handler/UIHandler3.ashx");
    objCallBack.OnComplete = function (rt, rx) {
        if (eval(rt) == -1) {
            stMin = 3;
        } else {
            stMin = eval(rt);
        }
        checklog();

    }



    function checklog() {
        var timeout = ''
        timeout = 120; // second

        intt = setInterval(function () {
            var str = document.cookie;
           // debugger;
            if (str.indexOf('Check') <= 0) {
                __doPostBack("Logout", UserID + "-" + ClientID);
            }
            var now = new Date();
            var diff = now - document.now;
            var time;
            time = parseFloat((diff / 1000) / 60)
            if (time >= stMin) {
                clearInterval(intt);
                var msg = "We found you are inactive long time and Your session will expire in {0} seconds?";
                var confirm = new $$.UI.WebControls.DialogBox();
                //      confirm.Message = "you haven't made any changes in a while. to protect your account's security you will be automatically signed out in {0} seconds?";
                confirm.Message = $$.Utility.StringFormat(msg, [timeout]);
                confirm.HeaderTitle = "Are you Still there?";
                confirm.YesButtonName = 'SignOut';
                confirm.NoButtonName = 'Continue';
                confirm.InterChangeYesNoButtonStyle = true;
                confirm.ComfirmBox(function () {
                    clearInterval(tnk);
                    document.cookie = "Talentoz=; expires=Thu, 01 Jan 1970 00:00:00 UTC";
                    if (typeof (Storage) !== "undefined") {
                        try {

                            localStorage.removeItem("LogOut");
                            localStorage.removeItem("CurrentAction");

                        }
                        catch (ex) {


                        };

                        localStorage.setItem("LogOut", "LogOut");
                        localStorage.setItem("CurrentAction", "LogOut");

                    }
                    window.onbeforeunload = function () { };
                    __doPostBack("Logout", UserID + "-" + ClientID);
                }, function () {
                    checklog();
                    document.now = new Date();
                    clearInterval(tnk);
                })
                var tnk = '';
                var tick = timeout;
                tnk = setInterval(function () {
                    var tmsg = '';
                    if (tick > 0) {
                        tmsg = $$.Utility.StringFormat(msg, [tick]);
                        $$("divComfirm").innerContent(tmsg)
                        tick = tick - 1;
                    } else {
                        clearInterval(tnk);
                        document.cookie = "Talentoz=; expires=Thu, 01 Jan 1970 00:00:00 UTC";
                        if (typeof (Storage) !== "undefined") {
                            try {

                                localStorage.removeItem("LogOut");
                                localStorage.removeItem("CurrentAction");

                            }
                            catch (ex) {


                            };

                            localStorage.setItem("LogOut", "LogOut");
                            localStorage.setItem("CurrentAction", "LogOut");

                        }
                        __doPostBack("Logout", UserID + "-" + ClientID);

                    }

                }, 1000);

            }
        }, 1000);
    }


