        cm.Mode = 0;
        cm.Context = 3;
        cm.Back = function () {
            fnCompList();
        }

        if (cm.ComponentID == 30000) {
            var tb = $$.TalentOZ.Tabs.CreateTab({ 'ID': 'comp_' + cm.ComponentID, 'TabName': cm.title })
        }
        else {
            var tb = $$.TalentOZ.Tabs.CreateTab({ 'ID': 'cmp_' + cm.ComponentID, 'TabName': cm.title })
        }

        cm.tb = tb;
        cm.tb.Load();
        cm.AfterSave = function () {
            switch (cm.ComponentID) {
                case 300000: OrgStruct.BindBusinessUnits(ClientID);
                case 310000: OrgStruct.BindWorkLocation(ClientID);
                case 320000: OrgStruct.BindDepartments(ClientID);
                case 340000: OrgStruct.BindGradeList(ClientID);
                case 350000: OrgStruct.BindJobList(ClientID);
                case 360000: OrgStruct.BindPositionList(ClientID);
            }
        }
        cm.Bind(tb.property.containerid);
    }

    //Password Information and Details

    function PassowordInformation(plc) {
        var str = '';
        objCallBack.DoCallBack("GetPasswordSettings", ClientID, "/" + virtualPath + "Handler/DataZieHandler.ashx");
        objCallBack.OnComplete = function (rs, rt) {
            if (rs != "" && rs != undefined) {
                var data = $.parseJSON(rs);
                $.each(data.PasswordSettings, function (obj) {
                    switch (data.PasswordSettings[obj].Key) {
                        case "STRENGTH":
                            if (data.PasswordSettings[obj].Value == 2) {
                                str = "<b> Password Strength :</b>  Include Alphanumeric characters " + "<br>";
                            } else if (data.PasswordSettings[obj].Value == 3) {
                                str += "<b> Password Strength :</b>  Include Alphanumeric  and Special characters" + "<br>";
                            } else
                                str += "<b> Password Strength :</b>  Any characters" + "<br>";
                            break;
                        case "LENGTH":
                            var value = "";
                            if (data.PasswordSettings[obj].Value == 0) { value = 3 }
                            else if (data.PasswordSettings[obj].Value == 1) { value = 4 }
                            else if (data.PasswordSettings[obj].Value == 2) { value = 5 }
                            else if (data.PasswordSettings[obj].Value == 3) { value = 6 }
                            else if (data.PasswordSettings[obj].Value == 4) { value = 7 }
                            else if (data.PasswordSettings[obj].Value == 5) { value = 8 }
                            else if (data.PasswordSettings[obj].Value == 6) { value = 9 }
                            else if (data.PasswordSettings[obj].Value == 7) { value = 10 }
                            str += "<b> Password Length :</b> Password Length must " + value + " characters";
                            break;
                    }
                });

                $$(plc).innerContent(str);
            }


        }
    }
