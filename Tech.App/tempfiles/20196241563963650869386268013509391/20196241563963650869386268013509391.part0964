INSERT INTO `pr_it_section`(`ClientID`,`FinancialYearID`,`TaxationType`,`SectionID`,`SectionCode`,`SectionName`,`SectionDescription`,`HelpText`,`RemarksText`,`MaxAmount`,`LastUPD`,`SectionOrder`) VALUES
(0,0,1,7,'80E','DEDUCTION UNDER SECTION 80 E','','<table class=\"TFtableCol\">\r\n                <thead>\r\n                <tr>\r\n                        <th colspan=\"2\">80 E\r\n                        </th>\r\n                    </tr>\r\n                </thead>\r\n                    <tbody>\r\n                    <tr>\r\n                        <th>General</th>\r\n                        <td>Deduction is available for max of 8 years starting the year of repayment\r\n                        </td>\r\n                    </tr>\r\n                    <tr>\r\n                        <th>Education Loan</th>\r\n                        <td>Scanned copy of interest certificate from bank .If your bank gives till date interest\r\n                            only you can get one more certificate in March before financial year completion\r\n                            with complete interest details for the full year (and claim within the allotted\r\n                            cutoff date which will be announced for March).In case the bank gives interest certificate\r\n                            for the remaining period please mention this clearly in your remarks.Then the difference\r\n                            interest in March can be updated in march (total interest for the year less already\r\n                            updated interest)</td>\r\n                    </tr>\r\n                </tbody></table>','Enter Remarks for SECTION 80 E',-1.00,'2019-04-10 19:56:37',7);
INSERT INTO `pr_it_section`(`ClientID`,`FinancialYearID`,`TaxationType`,`SectionID`,`SectionCode`,`SectionName`,`SectionDescription`,`HelpText`,`RemarksText`,`MaxAmount`,`LastUPD`,`SectionOrder`) VALUES
(0,0,1,9,'80U','DEDUCTION UNDER SECTION 80U','Submit Self-disability - Certificate in form 10 IA from prescribed medical authority with percentage of Disability','<table class=\"TFtableCol\">\r\n                    <tbody><tr>\r\n                        <th colspan=\"2\" style=\"margin: 0px; padding: 5px; text-align: left; color: rgb(85, 85, 85); line-height: 24px; font-family: arial; font-size: 14px; font-weight: bold;\">80 U\r\n                        </th>\r\n                    </tr>\r\n\r\n                 <tr>\r\n\r\n                      <td class=\"xl67\" >Self-disability<font class=\"font6\"><span style=\"mso-spacerun: yes\">&nbsp;</span></font></td>\r\n                        <td class=\"xl68\" >Certificate in <font class=\"font7\"><strong><span style=\"text-decoration: underline\">Form 10 IA</span></strong></font><font class=\"font6\"> from prescribed medical authority\r\n                                    with percentage of Disability.</font></td>\r\n                    </tr>\r\n                </tbody></table>','Enter Remarks for SECTION 80 U',125000.00,'2019-04-10 19:56:37',9);
INSERT INTO `pr_it_section`(`ClientID`,`FinancialYearID`,`TaxationType`,`SectionID`,`SectionCode`,`SectionName`,`SectionDescription`,`HelpText`,`RemarksText`,`MaxAmount`,`LastUPD`,`SectionOrder`) VALUES
(0,0,1,10,'24','DEDUCTION UNDER SECTION 24','','<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" class=\"TFtableCol\">\r\n                <thead>\r\n                <tr>\r\n                        <th colspan=\"3\">House Property\r\n                        </th>\r\n                    </tr>\r\n                </thead>\r\n                    <tbody>\r\n                    \r\n                    <tr>\r\n                        <th>\r\n                            Please note\r\n                        </th>\r\n                        <td>Benefit under Sec 24 can be claimed only if the property construction is completed\r\n                                    and possession has been taken by the claimant. If it is under construction INTEREST\r\n                                    benefit cannot be claimed, however you can claim the principal/registration/stamp\r\n                                    duty irrelevant of the possession status. Benefits will be available only if you\r\n                                    are sole/joint owner of the property (property registration should be in your name).\r\n                                    No Deduction of interest or principal can be claimed for loan taken for purchase\r\n                                    of vacant land.\r\n                        </td>\r\n\r\n                    </tr>\r\n                    \r\n                    <tr>\r\n                        <th colspan=\"2\">\r\n                           In case of Joint Ownership\r\n                        </th>\r\n\r\n                    </tr>\r\n                    \r\n                    <tr>\r\n                        <th>\r\n                            If the ownership proportion is mentioned in the agreement\r\n                        </th>\r\n                        <td>\r\n                           Tax exemption will be based on this proportion.\r\n                        </td>\r\n                      \r\n                    </tr>\r\n                    <tr>\r\n                        <th>\r\n                           If the ownership proportion is not mentioned in the agreement\r\n                        </th>\r\n                        <td>\r\n                           Equally split and only 50% will be allowed to the employee.\r\n                        </td>\r\n                        \r\n                    </tr>\r\n                    <tr>\r\n                        <th>\r\n                           If the ownership proportion is not \r\n                                    mentioned in the agreement and employee wants to claim in full or more than 50%\r\n                          \r\n                        </th>\r\n                        <td>\r\n                           Scanned copy of declaration from other co-owner(s) stating very clearly that they\r\n                                    will not claim the benefit in their assessment (Declaration Template attached with\r\n                                    this mail).\r\n                          \r\n                        </td>\r\n                       \r\n                    </tr>\r\n                    \r\n                    <tr>\r\n                        <th>\r\n                           Documents required for claiming tax\r\n                                    benefits on housing loan:\r\n                        </th>\r\n                        <td>\r\n                            \r\n                        </td>\r\n                        \r\n                    </tr>\r\n                    \r\n                    <tr>\r\n                        <th>\r\n                         Registration /stamp charges\r\n                        </th>\r\n                        <td>\r\n                           Scanned copy of receipt from registrar office if stamp duty is paid at registrar\r\n                                    office or scanned copy of agreement showing stamp value in case stamp duty is paid\r\n                                    by purchasing stamp paper.\r\n                        </td>\r\n                        \r\n                    </tr>\r\n\r\n                    <tr>\r\n                        <th>\r\n                          Principal/Interest Paid\r\n                        </th>\r\n                        <td>\r\n                          Scanned copy of certificate from bank.\r\n                                   \r\n                        </td>\r\n                       \r\n                    </tr>\r\n                    <tr>\r\n                        <th>Proof for Possession of House\r\n                        </th>\r\n                        <td>\r\n                           Scanned copy of handing over document from builder or electricity/water bill or\r\n                                    property tax receipt which is required to check if property is fit for occupation\r\n                                    and sale deed copy. Even if you have submitted these documents last year, you need\r\n                                    to submit this year also for audit verification as each year’s record is separately\r\n                                    kept.\r\n                        </td>\r\n                       \r\n                    </tr>\r\n                    <tr>\r\n                        <th rowspan=\"2\">\r\n                       Statement of computation\r\n                        </th>\r\n                        <td>\r\n                           If you have one property which is self-occupied please fill self-occupied online\r\n                                    template in IT declaration window. If you have one /more property/ies which\r\n                                    is let out please fill rented property online template separately for each property.For\r\n                                    let out property please arrange to submit copy of the lease agreement with the tenant\r\n                                    to verify the rent realized by you in respect of each property for which benefit\r\n                                    is claimed by you\r\n                        </td>\r\n\r\n                    </tr>\r\n                    <tr>\r\n                        <td>\r\n                            If you have one self-occupied and one let out please fill both templates. While uploading scanned proof please\r\n                                    name the proof like “bankstateselfoccupied” to refer to bank statement for self-occupied\r\n                                    property or bankstatrented1 to refer to bank statement for let out house 1 and so\r\n                                    that it is easy to approve for the person verifying the proof.\r\n                        </td>\r\n                       \r\n                    </tr>\r\n                </tbody></table>','',-1.00,'2019-04-10 19:56:37',10);
