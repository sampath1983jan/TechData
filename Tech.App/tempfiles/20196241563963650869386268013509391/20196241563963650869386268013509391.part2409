INSERT INTO `sys_emailtemplate`(`ClientID`,`ModuleID`,`TemplateID`,`Subject`,`Body`,`From`,`Signature`,`LastUPD`,`TemplateName`) VALUES
(100000,1,127,'Application received and review in process','<table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" width=\"100%\" style=\" font-family:arial;font-size:14px;line-height:22px;text-align:justify;color:#333; \">\r\n\r\n          <tr>\r\n              <td valign=\"top\" style=\"background:#fff;\">\r\n                 <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" width=\"100%\"   style=\"padding:30px;\">\r\n                 <tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px; font-family:arial; font-weight:bold; color:#333333;\">Hi #ApplicantName#,</td>\r\n                 </tr>\r\n                 <tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px;font-family:arial;line-height:20px;text-align:justify; font-weight:normal; color:#333333;\">\r\n                \r\n                 Thank you for visiting Careers at #CompanyName# and expressing interest in our current employment opportunities. This note is being sent to confirm that we have received your resume submission for the position of #OpeningTitle# - #WorkLocationName# and that it has been provided to our Recruiting team for consideration.</td>\r\n                 </tr>\r\n                 <tr><td style=\"height:20px;\"></td></tr>\r\n				 \r\n				  <tr>\r\n                 <td style=\"font-size:14px;font-family:arial; line-height:20px;text-align:justify;font-weight:normal; color:#333333;\">\r\n                \r\n                 Our process is simple. We look through the incoming resumes to see which ones best match our open jobs and contact the people we would like to learn more about. Unfortunately, we are unable to personally speak with everyone that sends us a resume, but please do not be discouraged if you do not hear from us. </td>\r\n                 </tr>\r\n				 <tr><td style=\"height:20px;\"></td></tr>\r\n				 <tr>\r\n                 <td style=\"font-size:14px;font-family:arial; line-height:20px;text-align:justify;font-weight:normal; color:#333333;\">\r\n                \r\n                We appreciate your interest in #CompanyName# and hope that you will visit our site again soon! </td>\r\n                 </tr>\r\n				 </table>\r\n\r\n				</td> \r\n				 \r\n				 \r\n			</tr>	 \r\n\r\n </table>','helpdesk@hrdpower.com','','2016-06-09 11:35:23','New Applied Applicant');
