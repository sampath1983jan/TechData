-- 
-- Definition of pr_bonus_config
-- 

CREATE TABLE IF NOT EXISTS `pr_bonus_config` (
  `ClientID` int(10) unsigned NOT NULL,
  `EmployeeGroupID` int(10) unsigned NOT NULL COMMENT 'Configuartion is based on Employee Group',
  `BonusConfigurationName` varchar(500) NOT NULL COMMENT 'Configuartion Name',
  `BonusApplicable` tinyint(1) NOT NULL DEFAULT '1',
  `BonusEligibilityDays` int(11) NOT NULL COMMENT 'Minimum days to get bonus eligibility',
  `BonusWagesFormula` varchar(500) NOT NULL COMMENT 'Components need to include in the  Bonus',
  `BonusEligibilityAmount` decimal(12,2) NOT NULL COMMENT 'Bonus is eligible when he is getting less than or equal this amount',
  `ConsiderMaxWages` tinyint(1) NOT NULL DEFAULT '1',
  `MaxWages` decimal(12,2) NOT NULL COMMENT 'The maximum wages to consider for  amount',
  `ConsiderMinWagesonLOP` tinyint(1) NOT NULL DEFAULT '0',
  `MinWages` decimal(12,2) NOT NULL COMMENT 'Minimum Wages',
  `BonusPercentage` decimal(10,2) NOT NULL,
  `Bonus_NA_EmploymentStatus` varchar(45) DEFAULT NULL COMMENT 'can save bonus not applicable multilple employment status',
  `Bonus_NA_Employees` varchar(500) DEFAULT NULL COMMENT 'can save multiple not applicable employees',
  `ExgratiaApplicable` tinyint(1) NOT NULL DEFAULT '0',
  `ExgratiaWagesFormula` varchar(500) DEFAULT NULL COMMENT 'Formula should  Minus the Bonus amount',
  `ExgratiaPercentage` varchar(45) DEFAULT NULL,
  `Exgratia_NA_Employmentstatus` varchar(45) DEFAULT NULL COMMENT 'can save not applicable multiple employment status',
  `Exgratia_NA_Employees` varchar(500) DEFAULT NULL COMMENT 'can save exgratia not applicable employees',
  `CreatedBy` int(10) unsigned NOT NULL,
  `LastUPD` datetime NOT NULL,
  PRIMARY KEY (`ClientID`,`EmployeeGroupID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

