_title h3 {\r\n                font-size: 0.9rem;\r\n                color: #333;\r\n                text-align: center;\r\n            }\r\n\r\n            .hed_title h4 {\r\n                font-size: 0.9rem;\r\n                color: #999;\r\n            }\r\n\r\n            .hed_title h5 {\r\n                font-size: 0.7rem;\r\n                color: #333;\r\n                text-align: center;\r\n            }\r\n\r\n            .hed_title h6 {\r\n                font-size: 0.8rem;\r\n                color: #333;\r\n                padding: 10px;\r\n                font-weight: normal;\r\n            }\r\n\r\n        .bdy_cont {\r\n            border: 1px solid #333;\r\n        }\r\n\r\n            .bdy_cont td {\r\n                padding: 5px;\r\n                font-size: 0.8rem;\r\n            }\r\n\r\n            .bdy_cont th {\r\n                padding: 10px 5px;\r\n                font-size: 0.9rem;\r\n                font-weight: bold;\r\n            }\r\n\r\n        .brd_lft {\r\n            border-left: 1px solid #333;\r\n        }\r\n\r\n        .brd_rht {\r\n            border-right: 1px solid #333;\r\n        }\r\n\r\n        .brd_bot {\r\n            border-bottom: 1px solid #333;\r\n        }\r\n\r\n        .brd_top {\r\n            border-top: 1px solid #333;\r\n        }\r\n\r\n        .txt_lft {\r\n            text-align: left;\r\n        }\r\n\r\n        .txt_cnt {\r\n            text-align: center;\r\n        }\r\n\r\n        .txt_rht {\r\n            text-align: right;\r\n        }\r\n\r\n        .pad0 {\r\n            padding: 0px;\r\n        }\r\n\r\n        .pad20_b {\r\n            padding-bottom: 20px;\r\n        }\r\n    </style>\r\n</head>\r\n\r\n<body>\r\n    <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" width=\"800px\" class=\"tablebody\">\r\n        <tr>\r\n            <td class=\"tdbody\">\r\n                <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" width=\"100%\" class=\"hed_title pad20_b\">\r\n       