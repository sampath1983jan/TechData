p(SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_NONE)\r\n                    .AddWhereClause(0, \"Sys_User\", \"ClientID\", SMRHRT.Data.FieldType.FLD_NUMBER, SMRHRT.Data.SQLFieldOperators.FLD_EQUAL, CStr(Me.ClientID), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_AND)\r\n                    .AddWhereClause(0, \"Sys_User\", \"UserID\", SMRHRT.Data.FieldType.FLD_NUMBER, SMRHRT.Data.SQLFieldOperators.FLD_EQUAL, CStr(Me.UserID), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_NONE)\r\n                End With\r\n                dtResult = DB.GetDataTable(mSqlBuilder)\r\n                ''Dim mPrc As New Procedure\r\n                ''Dim mParameter As MySqlParameter() = New MySqlParameter(3) {}\r\n                ''mParameter(0) = New MySqlParameter(\"pClientID\", MySqlDbType.Int32)\r\n                ''mParameter(0).Value = Me.ClientID.ToString\r\n                ''mParameter(1) = New MySqlParameter(\"pUserID\", MySqlDbType.String)\r\n                ''mParameter(1).Value = Me.UserID.ToString\r\n                ''dtResult = mPrc.GetDataTable(\"test_Sample2\", mParameter)\r\n            Catch ex As Exception\r\n                Throw ex\r\n            End Try\r\n            If dtResult.Rows.Count > 0 Then\r\n                Return CInt(dtResult.Rows(0)(0))\r\n            Else\r\n                Return -1\r\n            End If\r\n        End Function\r\n\r\n        Public Function GetEmployeeSlabValue(ByVal pLookupItemID As Integer) As Decimal\r\n            Dim dtResult = New DataTable\r\n            Try\r\n                Dim mSqlBuilder = New SMRHRT.Data.SQLBuilder(SMRHRT.Data.SQLType.SQL_SELECT, DB)\r\n                With mSqlBuilder\r\n                    .AddTable(\"pr_salary_component_slab\")\r\n                    .AddField(\"pr_salary_component_slab\", \"SlabValue\")\r\n                    .AddWhereGroup(SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_NONE)\r\n                    .AddWhereClause(0, \"pr_salary_component_slab\", \"ClientID\", SMRHRT.Data.FieldType.FLD_NUMBER, SMRHRT.Data.SQLFieldOperators.FLD_EQUAL, CStr(Me.ClientID), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_AND)\r\n                    .AddWhereClause(0, \"pr_salary_component_slab\", \"ComponentID\", SMRHRT.Data.FieldType.FLD_NUMBER, SMRHRT.Data.SQLFieldOperators.FLD_EQUAL, CStr(Me.ComponentID), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_AND)\r\n                    .AddWhereClause(0, \"pr_salary_component_slab\", \"LookupItemID\", SMRHRT.Data.FieldType.FLD_NUMBER, SMRHRT.Data.SQLFieldOperators.FLD_EQUAL, CStr(pLookupItemID), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_AND)\r\n                    .AddWhereClause(0, \"pr_salary_component_slab\", \"RangeFrom\", SMRHRT.Data.FieldType.FLD_DECIMAL, SMRHRT.Data.SQLFieldOperators.FLD_LESSEQUAL, CStr(Me.RangeComponentValue), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_AND)\r\n                    .AddWhereClause(0, \"pr_salary_component_slab\", \"RangeTo\", SMRHRT.Data.FieldType.FLD_DECIMAL, SMRHRT.Data.SQLFieldOperators.FLD_GREATEREQUAL, CStr(Me.RangeComponentValue), SMRHRT.Data.SQLWhereJoinType.SQL_WHERE_NONE)\r\n                End With\r\n                dtResult = DB.GetDataTable(mSqlBuilder)\r\n            Catch ex As Exception\r\n                Throw ex\r\n            End Try\r\n            If dtResult.Rows.Count > 0 Then\r\n                Return CInt(dtResult.Rows(0)(0))\r\n            Else\r\n                Return -1\r\n            End If\r\n        End Function\r\n    End Class\r\nEnd Namespace~CountryPTSlab','2017-02-01 11:56:47',1,0,-1,0,NULL);
