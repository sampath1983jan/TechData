INSERT INTO `sys_emailtemplate`(`ClientID`,`ModuleID`,`TemplateID`,`Subject`,`Body`,`From`,`Signature`,`LastUPD`,`TemplateName`) VALUES
(100000,1,2000,'Your Profile has been rejected','\r\n                    <tr>\r\n                        <td style=\"background:#fff;\" colspan=\"4\">\r\n                            <table width=\"100%\" cellpadding=\"0\" border=\"0\" cellspacing=\"0\" style=\"background:#ffffff;\" bgcolor=\"#ffffff\">\r\n                                <tr>\r\n                                    <td style=\"height:20px;\" height=\"20px\" colspan=\"3\"></td>\r\n                                </tr>\r\n\r\n                                <tr>\r\n                                    <td style=\"width:20px;\" width=\"20px\"></td>\r\n\r\n                                    <td style=\"display:block; text-align:left;\" align=\"left\" valign=\"top\">\r\n                                        <table width=\"100%\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n                                            <tbody>\r\n                                                <tr><td style=\"height: 10px;\"></td></tr>\r\n                                                 \r\n\r\n\r\n												 <tr>\r\n												 \r\n<td valign=\"top\" style=\"background:#fff; padding:0px 10px;\">\r\n                 <table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\r\n                 <tbody><tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px; font-family:Arial; font-weight:bold; color:#333333;\">Dear #CandidateName#,</td>\r\n                 </tr>\r\n                 <tr><td style=\"height:20px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px;font-family:arial; font-weight:normal;line-height:24px; color:#333333;text-align: justify;\">We reviewed your application for the #PositionTitle#  at #CompanyName#  and, after looking at all of the applications received, think this might not be the best fit. We encourage you to look at the other opportunities we offer to put potential to work.\r\nWe realize you may be disappointed. But we deeply appreciate you taking the time to apply. We would also like to know if there is anything you believe we could have done to make your experience as a candidate better.</td>\r\n                 </tr>\r\n                 <tr><td style=\"height:20px;\"></td></tr>\r\n				 \r\n                 <tr>\r\n                 <td style=\"font-size:14px;font-family:arial; font-weight:normal; color:#333333;\">\r\n				 We wish you all the best for your career. Thank you once again for your interest in  #CompanyName#</td></tr>\r\n				 \r\n				 <tr><td style=\"height:20px;\"></td></tr>\r\n					\r\n					 \r\n                    \r\n                    \r\n                    \r\n                 </tbody></table>\r\n              </td>\r\n												 </tr>\r\n\r\n\r\n\r\n\r\n                                                 \r\n\r\n                                            </tbody>\r\n                                        </table>\r\n\r\n\r\n                                    </td>\r\n                                    <td style=\"width:20px;\" width=\"20px\"></td>\r\n                                </tr>\r\n\r\n                                <tr>\r\n                                    <td style=\"height:20px;\" height=\"20px\" colspan=\"3\"></td>\r\n                                </tr>\r\n                            </table>\r\n\r\n                        </td>\r\n                    </tr>','helpdesk@hrdpower.com','','2016-07-19 15:46:37','Your Profile has been rejected');
INSERT INTO `sys_emailtemplate`(`ClientID`,`ModuleID`,`TemplateID`,`Subject`,`Body`,`From`,`Signature`,`LastUPD`,`TemplateName`) VALUES
