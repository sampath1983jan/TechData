(100000,179,'EXPERIENCE BONUS',0,'EXPERIENCE_BONUS',0,0,1,1,3,0,'',0,'','Imports MySql.Data.MySqlClient\r\nImports System\r\nImports System.Data\r\nImports System.Runtime.InteropServices\r\nImports TalentOZNet.Global\r\nImports SMRHRT.Data\r\nImports SMRHRT.Services.Collections.ErrorCollection\r\nNamespace Talentoz.DynamicCode.PayRoll\r\n\r\n    Public Class ExperienceBonus\r\n        Inherits CustomComputer\r\n\r\n        Public Overrides Function GetValue() As Decimal\r\n\r\n            Dim objD = New AttendanceBonus_D\r\n\r\n            objD.ClientID = Me.ClientID\r\n            objD.UserID = Me.UserID\r\n            objD.ComponentID = Me.ComponentID\r\n            objD.Month = Me.Month\r\n            objD.Year = Me.Year\r\n            objD.PayCycleID = Me.PayCycleID\r\n            objD.FinancialYearID=Me.FinancialYearID\r\n\r\n            Return objD.GetEmployeeExperienceBonus()\r\n\r\n        End Function\r\n    End Class\r\n\r\n    Public Class ExperienceBonus_D\r\n        Inherits Core.BaseDataAccess\r\n        Dim dtResult As DataTable\r\n        Public Property ClientID As Integer\r\n        Public Property ComponentID As Integer\r\n        Public Property UserID As Integer\r\n        Public Property Month As Integer\r\n        Public Property Year As Integer\r\n        Public Property PayCycleID As Integer\r\n        Public Property FinancialYearID As Integer\r\n\r\n\r\n#Region \"Constructor\"\r\n        Public Sub New()\r\n            MyBase.New()\r\n            Try\r\n                MyBase.InitDB()\r\n            Catch dEx As SMRHRT.Data.DataException\r\n                Throw\r\n            End Try\r\n        End Sub\r\n#End Region\r\n\r\n        Public Function GetEmployeeExperienceBonus() As Decimal\r\n            Try\r\n                Dim dProc As New Procedure\r\n                Dim Result As Decimal = 0\r\n\r\n                    Dim parameter As MySqlParameter() = New MySqlParameter(4) {}\r\n                    parameter(0) = New MySqlParameter(\"pClientID\", MySqlDbType.Int32)\r\n                    parameter(0).Value = Me.ClientID.ToString\r\n                    parameter(1) = New MySqlParameter(\"pUserID\", MySqlDbType.Int32)\r\n                    parameter(1).Value = Me.UserID.ToString\r\n                    parameter(2) = New MySqlParameter(\"pMonth\", MySqlDbType.Int32)\r\n                    parameter(2).Value = Me.Month.ToString\r\n                    parameter(3) = New MySqlParameter(\"pYear\", MySqlDbType.Int32)\r\n                    parameter(3).Value = Me.Year.ToString\r\n                    parameter(4) = New MySqlParameter(\"pComponentID\", MySqlDbType.Int32)\r\n                    parameter(4).Value = Me.ComponentID.ToString\r\n                    Result = dProc.ExecuteFunction(\"FN_PR_GetEmployeeExperienceBonus\", parameter)\r\n                Return Result\r\n            Catch dEx As SMRHRT.Data.DataException\r\n                Throw dEx\r\n            End Try\r\n        End Function\r\n\r\n    End Class\r\nEnd Namespace~ExperienceBonus','2017-11-07 16:05:42',0,0,-1,0,65,1,0,NULL,'EXPERIENCE_BONUS',NULL,0);
