(4,3,503,' Subordinate(s) Performance Evaluation - Published','<table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" width=\"100%\">\n    <tbody>\n        <tr><td style=\"height:10px;\"></td></tr>\n        <tr>\n            <td style=\"font-size:14px; font-family:Arial; font-weight:bold; color:#333333;\">Dear {ReceiverName},</td>\n        </tr>\n        <tr><td style=\"height:10px;\"></td></tr>\n        <tr><td style=\"height:10px;\"></td></tr>\n        <tr>\n            <td style=\"font-size:12px; font-family:Arial; font-weight:normal; color:#333333;line-height:20px;\">This is to inform you that, Performance Evaluation for your subordinate(s) has been published, pending for your preparation. Please complete the Form Preparation by {FormPreparationEndDate}.<br /><br />Please find the Performance Evaluation details below.\n</td>\n        </tr>\n        <tr><td style=\"height:20px;\"></td></tr>\n        <tr>\n            <td style=\" font-size:12px; font-family:Arial; font-weight:normal; color:#333333;\"></td>\n        </tr>\n        <tr>\n            <td style=\"font-size:12px; font-family:Arial; font-weight:normal; color:#333333;\">\n                <table width=\"100%\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\"><tr><td height=\"20px\" style=\"border-top:1px solid #ccc\"></td><tr></table>\n            </td>\n        </tr>\n        <tr>\n            <td style=\" font-size:12px; font-family:Arial; font-weight:normal; color:#333333;\">\n                <table cellpadding=\"0\" cellspacing=\"0\" border=\"0\" width=\"100%\" style=\"background-color:#f5f5f5\">\n                    <tbody>\n\n                        <tr><td width=\"22%\" align=\"left\" style=\"padding:5px; text-align:left; background:#fff;font-size:12px; color:#333333; font-weight:bold; line-height:20px;font-family:arial;\">Performance Title:</td>\n                        <td align=\"left\" colspan=\"3\" style=\"padding:5px; text-align:left; font-size:12px; color:#333333; font-weight:normal; line-height:20px;font-family:arial; background:#fff;\">{PerformancePlanTitle}</td>\n                    \n                        <tr><td width=\"22%\" align=\"left\" style=\"padding:5px; text-align:left; font-size:12px; color:#333333; background:#fff; font-weight:bold; line-height:20px;font-family:arial;\">Performance Evaluation Period From :</td>\n                        <td align=\"left\" style=\"padding:5px; text-align:left; font-size:12px; color:#333333; font-weight:normal; line-height:20px;font-family:arial; background:#fff;\">{StartDate}</td>\n                        <td width=\"22%\" align=\"left\" style=\"padding:5px; text-align:left; font-size:12px; color:#333333; font-weight:bold; line-height:20px;font-family:arial; background:#fff;\">Performance Evaluation Period Till :</td>\n                        <td align=\"left\" style=\"padding:5px; text-align:left; font-size:12px; color:#333333; font-weight:normal; line-height:20px;font-family:arial; background:#fff;\">{EndDate}</td></tr>\n\n                    </tbody>\n                </table>\n            </td>\n        </tr>\n        <tr><td style=\"height:20px;\"></td></tr>\n\n\n\n    </tbody>\n</table> \n','help@hrdpower.com','1','2014-09-02 19:05:54','Appraisal Publish to Manager');
