INSERT INTO `sys_emailtemplate`(`ClientID`,`ModuleID`,`TemplateID`,`Subject`,`Body`,`From`,`Signature`,`LastUPD`,`TemplateName`) VALUES
(100000,3,21,'Email Notification to Manager when employee rejected the manager scoring','\r\n<html xmlns=''http://www.w3.org/1999/xhtml''>\r\n<head>\r\n    <meta http-equiv=''Content-Type'' content=''text/html; charset=iso-8859-1'' />\r\n    <title>TalentOZ - Emailtemplate</title>\r\n</head>\r\n<body>\r\n    <div style=''padding:0px; margin:0px; position:relative; font-family:Arial; width:800px; text-align:left; font-size:14px; color:#999;''>\r\n        <div style=''background:#00adef; height:120px; padding:25px 20px; position:relative;''>\r\n            <img src=''http://hrdpower.com/talentOZ_logo.png'' alt=''TalentOZ'' /><div style=''position:absolute; right:40px; top:20px; color:#fff; font-weight:bold;''>{EmailDate}</div>\r\n        </div>\r\n        <div style=''background-color:#f4f4f4; height:625px; position:relative; padding:25px 20px;''>\r\n            <div style=''position:absolute; top:-50px; left:35px; background-color:#fff; width:685px; padding:20px;''>\r\n                <div style=''position:absolute; top:0px; left:0px;''>\r\n                    <span style=''background:#386898; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#eee; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#e02e2e; height:5px; width:60px; display:block;\r\n\r\nfloat:left;''></span><span style=''background:#eee; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#386898; height:5px;\r\n\r\nwidth:60px; display:block; float:left;''></span><span style=''background:#eee; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#e02e2e; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#eee; height:5px; width:60px; display:block;\r\n\r\nfloat:left;''></span><span style=''background:#386898; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#eee; height:5px;\r\n\r\nwidth:60px; display:block; float:left;''></span><span style=''background:#e02e2e; height:5px; width:60px; display:block; float:left;''></span><span style=''background:#eee; height:5px; width:65px; display:block; float:left;''></span>\r\n                </div>\r\n                <div style=''font-size:16px; padding:15px 0px; font-weight:bold;''><b>Dear</b> <span style=''color:#00adef;''>{ReceiverName},</span></div>\r\n                <div style=''font-size:16px; color:#666; border-bottom:1px dotted #ccc; padding-bottom:10px; font-weight:bold; ''>\r\n                    {subject}\r\n                </div>\r\n                <div style=''padding:15px 0px; line-height:24px; ''>\r\n                    This is to inform you that, your subordinate(s) has rejected your review score for the {PerformancePlanTitle} performance review. Please find the review details below.\r\n                </div>\r\n                <div style=''padding:15px; line-height:24px; background-color:#eee; color:#666; border-radius:5px;''>\r\n                    <b>Performance Title  : </b>{PerformancePlanTitle}<br />\r\n                    <b>Performance Description : </b> {PerformanceDescription}<br />\r\n                    <div style=''padding-top:10px;''>\r\n                        <div style=''float:left;''><b>Effective From :</b> {StartDate}</div><div style=''float:right;''>\r\n                            <b>Effective Till :</b> {EndDate}\r\n                        </div><div style=''clear:both;''></div>\r\n                    </div>\r\n\r\n                </div>\r\n\r\n                <div style=''font-size:16px; padding:15px 0px; position:relative; ''>\r\n                    Please <b>\r\n                        <a href=''http://www.talentoz.com'' target=''_blank''\r\n                           style=''color:#00adef;''>login</a>\r\n                    </b> to the system and complete {PerformancePlanTitle}.\r\n                </div>\r\n                <div style=''padding:15px; text-align:center;''>\r\n                    <a href=''http://www.talentoz.com'' target=''_blank'' style=''padding:15px; font-weight:bold;\r\n\r\nbackground:#00adef; text-decoration:none; color:#fff; border:0px; border-radius:5px; cursor:pointer;''>Login Now</a>\r\n                </div>\r\n                <div style=''font-size:16px; padding:15px 0px; line-height:24px;''>Thank You !<br /> <b style=''color:#00adef;''>Talentoz</b> </div>\r\n            </div>\r\n\r\n            <div style=''color:#00adef; border-bottom:1px dotted #ccc; padding-bottom:10px; padding-top:480px; ''>\r\n                www.talentoz.com &nbsp;|&nbsp; Privacy Policy &nbsp;|\r\n\r\n                &nbsp; Terms of Use\r\n            </div>\r\n            <div style=''padding-top:10px; line-height:20px;''>\r\n                <b>Disclaimer:</b>  This is an auto generated message, please do not reply to this e-mail. All rights\r\n\r\n                reserved Talentoz, This message is confidential. It may also be privileged or otherwise protected by work product immunity or other legal rules. If you\r\n\r\n                havereceived it by mistake, please let us know by e-mail reply and delete it from your system; you may not copy this message or discloseits contents to\r\n\r\n                anyone. Please send us by fax any message containing deadlines as incoming e-mails are not screened for response\r\n                deadlines. The integrity and security of this message cannot be guaranteed on the Internet.\r\n            </div>\r\n        </div>\r\n    </div>\r\n</body>\r\n</html>','from','1','2017-07-03 18:02:49','Email Notification to Manager when employee rejected the manager scoring');
INSERT INTO `sys_emailtemplate`(`ClientID`,`ModuleID`,`TemplateID`,`Subject`,`Body`,`From`,`Signature`,`LastUPD`,`TemplateName`) VALUES
(100000,1,25,'Mail Send To Candidate for Interview Schedule','<table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\" style=\" font-family:arial;font-size:14px;line-height:22px;text-align:justify;color:#333; \">\r\n\r\n          <tbody><tr>            \r\n              <td valign=\"top\" style=\"background:#fff;\">\r\n                 <table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\r\n                 <tbody><tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px;font-family:arial; line-height:20px;text-align:justify;  font-weight:bold; color:#333333;\">Dear {Name}, </td>\r\n                 </tr>\r\n                 <tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px; font-family:arial; line-height:20px;text-align:justify;font-weight:normal; color:#333333;\">\r\n                \r\n                 We are pleased to inform that you have been shortlisted for the position {Position name}. Please find the below details for attending a Personal Interview.</td>\r\n                 </tr>\r\n                 <tr><td style=\"height:20px;\"></td></tr>\r\n\r\n\r\n				  \r\n                 <tr><td style=\"font-size:14px; font-weight:normal; color:#333333;\">\r\n      \r\n\r\n\r\n<table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\" style=\"margin:0; \">\r\n\r\n<tbody>\r\n\r\n<tr><td  valign=\"top\" style=\" text-align:left; font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Round Name: &nbsp; &nbsp;<b>{Round Name}</b></td></tr>\r\n<tr><td   valign=\"top\" height=\"20px\" style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Interview Date:  &nbsp; &nbsp;<b> {Interview Date}</b></td> </tr>\r\n \r\n \r\n\r\n\r\n\r\n\r\n\r\n<tr><td valign=\"top\"  style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Start Time:  &nbsp; &nbsp; <b>{Interview Time}</b></td></tr>\r\n<tr><td  valign=\"top\" style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> End Time:   &nbsp; &nbsp; <b>{InterviewEndTime}</b></td></tr>\r\n\r\n\r\n<tr>\r\n<td   valign=\"top\" style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Venue:   &nbsp; &nbsp; <b>{Venue}</b></td>\r\n</tr>\r\n \r\n \r\n</table>	\r\n\r\n	  \r\n\r\n \r\n\r\n                 \r\n                 </td>\r\n                 </tr> \r\n				 \r\n				 				 \r\n				 \r\n\r\n			  </tbody></table>\r\n              </td>\r\n\r\n		 \r\n                  \r\n          </tr>        \r\n </tbody></table>','helpdesk@hrdpower.com','','2016-07-28 14:18:27','Mail Send To Candidate for Interview Schedule');
INSERT INTO `sys_emailtemplate`(`ClientID`,`ModuleID`,`TemplateID`,`Subject`,`Body`,`From`,`Signature`,`LastUPD`,`TemplateName`) VALUES
(100000,1,26,'Mail Send To Evaluator for Interview Schedule','<table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\" style=\" font-family:arial;font-size:14px;line-height:22px;text-align:justify;color:#333; \">\r\n         \r\n          <tbody><tr>            \r\n              <td valign=\"top\" style=\"background:#fff;\">\r\n                 <table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\r\n                 <tbody><tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px;font-family:arial; line-height:20px;text-align:justify;  font-weight:bold; color:#333333;\">Dear {Name}, </td>\r\n                 </tr>\r\n                 <tr><td style=\"height:10px;\"></td></tr>\r\n                 <tr>\r\n                 <td style=\"font-size:14px; font-family:arial; line-height:20px;text-align:justify;font-weight:normal; color:#333333;\">\r\n                \r\n               We are pleased to inform that you have been assigned as an Evaluator for the position {position name}. Please find the below evaluation details.</td>\r\n                 </tr>\r\n                 <tr><td style=\"height:20px;\"></td></tr>\r\n				 \r\n				 \r\n\r\n                 <tr><td style=\"font-size:14px; font-weight:normal; color:#333333;\">\r\n\r\n\r\n<table width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\" style=\"margin:0; \">\r\n\r\n<tbody>\r\n\r\n<tr><td  valign=\"top\" style=\" text-align:left; font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Candidate Name: &nbsp; &nbsp;<b>{CandidateName}</b></td></tr>\r\n<tr><td   valign=\"top\" height=\"20px\" style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Email:  &nbsp; &nbsp;<b> {Email}</b></td> </tr>\r\n \r\n \r\n\r\n\r\n<tr><td   valign=\"top\" style=\"text-align:left; font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Round Name: &nbsp; &nbsp; <b>{Round Name}</b></td></tr>\r\n<tr><td  valign=\"top\" style=\"text-align:left; font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Evaluation Date:  &nbsp; &nbsp; <b>{Interview Date}</b></td></tr>\r\n\r\n\r\n<tr><td valign=\"top\"  style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Start Time:  &nbsp; &nbsp; <b>{Interview Time}</b></td></tr>\r\n<tr><td  valign=\"top\" style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> End Time:   &nbsp; &nbsp; <b>{EndTime}</b></td></tr>\r\n\r\n\r\n<tr>\r\n<td   valign=\"top\" style=\"  text-align:left;font-family:arial;font-size:14px; color:#555; margin:0;padding:5px 10px;\"> Venue:   &nbsp; &nbsp; <b>{Venue}</b></td>\r\n</tr>\r\n \r\n \r\n</table>				  \r\n                \r\n                 \r\n                 </td>\r\n                 </tr> \r\n				 \r\n				 \r\n				 <tr>\r\n                 <td colspan=\"5\" style=\"font